{
    "manifest_version": 3,
    "name": "RR Auto Click",
    "version": "1.0.0",
    "description": "Auto clicker for the RR platform",
    "icons": {
      "32": "images/icon32.png",
      "128": "images/icon128.png"
    },
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "action": {},
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}